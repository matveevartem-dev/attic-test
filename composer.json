{
    "name": "artem/inline-test",
    "description": "INLINE Test Project",
    "keywords": ["inline-test"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Matveev",
            "email": "dev.artem.matveev@gmail.com",
            "role": "Project developer"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "codeception/c3": true
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "ext-pdo_mysql": "*",
        "devanych/di-container": "^2.1",
        "doctrine/dbal": "4.4.x-dev",
        "doctrine/orm": "3.6.x-dev",
        "dunglas/doctrine-json-odm": "^1.4",
        "halaxa/json-machine": "^1.2",
        "httpsoft/http-basis": "^1.1",
        "monolog/monolog": "^2.3|^3.3",
        "php-di/php-di": "^7.0",
        "phpunit/phpunit": "9.6.x-dev",
        "salsify/json-streaming-parser": "^8.3",
        "symfony/cache": "8.0.x-dev",
        "symfony/console": "6.4.x-dev",
        "twig/twig": "4.x-dev",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "filp/whoops": "^2.15",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/var-dumper": "^5.4|^6.2",
        "vimeo/psalm": "^4.9|^5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@chmod-var"
        ],
        "post-update-cmd": [
            "@chmod-var"
        ],
        "serve": "php -S 0.0.0.0:8088 -t public/",
        "test": "phpunit --colors=always",
        "static": "psalm",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "check": [
            "@cs-check",
            "@static",
            "@test"
        ],
        "chmod-var": "php ./bin/chmod-var.php"
    }
}
